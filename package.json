{
  "name": "qti-xml-core",
  "version": "0.1.2",
  "description": "Minimal, dependency-free helpers for parsing QTI 3.0 XML with DOMParser.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metyatech/qti-xml-core.git"
  },
  "homepage": "https://github.com/metyatech/qti-xml-core",
  "bugs": {
    "url": "https://github.com/metyatech/qti-xml-core/issues"
  },
  "keywords": [
    "qti",
    "qti3",
    "xml",
    "assessment",
    "parser"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node --test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "verify": "npm run lint && npm run format:check && npm run typecheck && npm test",
    "prepack": "npm run build",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.9.8"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "^5",
    "typescript-eslint": "^8.54.0"
  }
}
